<div class="#">
  <h3>Welcome to dev kitchen!</h3>
  <p>Login below:</p>

  <div class="#">
    <%= form_with url: sessions_path, local: true do |f| %>

    <div class="#">
      Email: <%= f.text_field :email  %>
    </div>

    <div class="#">
      Password: <%= f.password_field :password %>
    </div>

    <div class="#">
      <%= f.submit "Log in!" %>
    </div>

    <% end %>
    <p><%= link_to "Create an account", new_user_path %></p>
  </div>
</div>
